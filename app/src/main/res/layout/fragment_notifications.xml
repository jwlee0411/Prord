<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".ui.notifications.NotificationsFragment">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#441b16"
        android:textSize="26sp"
        app:layout_constraintStart_toStartOf="@+id/button3"
        app:layout_constraintTop_toTopOf="parent"
        android:text="카테고리" />
    <Button
        android:id="@+id/button2"
        android:layout_width="145dp"
        android:layout_height="93dp"
        android:text="정보보안"
        android:textSize="0dp"
        android:background="@drawable/imgprotect"
        app:layout_constraintBottom_toTopOf="@id/button8"
        app:layout_constraintEnd_toStartOf="@id/button9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <Button
        android:id="@+id/button3"
        android:layout_width="145dp"
        android:layout_height="93dp"
        android:text="정보통신"
        android:textSize="0dp"
        android:background="@drawable/imgtech"
        app:layout_constraintEnd_toStartOf="@+id/button4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/button2"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/button4"
        android:layout_width="145dp"
        android:layout_height="93dp"
        android:text="공정거래"
        android:textSize="0dp"
        android:background="@drawable/imgfairtrade"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/button3"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/button5"
        android:layout_width="145dp"
        android:layout_height="93dp"
        android:text="행정업무"
        android:textSize="0dp"
        android:background="@drawable/imgwork"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button6"
        app:layout_constraintTop_toTopOf="@+id/button6" />

    <Button
        android:id="@+id/button6"
        android:layout_width="145dp"
        android:layout_height="93dp"
        android:text="건축"
        android:background="@drawable/imgconstruct"
        app:layout_constraintEnd_toStartOf="@id/button5"
        app:layout_constraintStart_toStartOf="parent"
        android:textSize="0dp"
        app:layout_constraintTop_toBottomOf="@+id/button8"
        app:layout_constraintBottom_toTopOf="@id/textView7"/>

    <Button
        android:id="@+id/button7"
        android:layout_width="145dp"
        android:layout_height="93dp"
        android:text="디자인"
        android:textSize="0dp"
        android:background="@drawable/imgdesign"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button8"
        app:layout_constraintTop_toTopOf="@+id/button8" />

    <Button
        android:id="@+id/button8"
        android:layout_width="145dp"
        android:layout_height="93dp"
        android:text="의류"
        android:textSize="0dp"
        android:background="@drawable/imgcloth"
        app:layout_constraintBottom_toTopOf="@id/button6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintEnd_toStartOf="@id/button7"/>

    <Button
        android:id="@+id/button9"
        android:layout_width="145dp"
        android:layout_height="93dp"
        android:text="하드웨어"
        android:textSize="0dp"
        android:background="@drawable/imghardware"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button2"
        app:layout_constraintTop_toTopOf="@+id/button2" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="모두가 찾는 분야"
        app:layout_constraintStart_toStartOf="@+id/button3"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button6"
        tools:layout_editor_absoluteX="56dp" />
</androidx.constraintlayout.widget.ConstraintLayout>